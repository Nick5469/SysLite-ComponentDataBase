{
    "name": "SysLite - Component Database",
    "version": "6.0.50723",
    "APIMinVersion": "6",
    "MinSupportVersion": "Windows Vista(5112)",
    "repo": "https://github.com/Nick5469/SysLite-ComponentDataBase",
    "itemList": [
        {
            "name": "组件名称",
            "description": "组件的内部名称或常用的表述形式。",
            "lock": true,
            "path": "name"
        },
        {
            "name": "显示名称",
            "description": "组件的译名。",
            "lock": false,
            "path": "display"
        },
        {
            "name": "组件描述",
            "description": "对组件功能的解释。",
            "lock": false,
            "path": "description"
        },
        {
            "name": "占用",
            "description": "组件的存储占用情况。",
            "lock": false,
            "path": "_size"
        },
        {
            "name": "架构",
            "description": "组件文件架构（显示'-'代表未知）。",
            "lock": false,
            "path": "arch.bit"
        },
        {
            "name": "兼容性",
            "description": "组件关联的兼容性信息",
            "lock": true,
            "path": "compatibilityList"
        },
        {
            "name": "依赖",
            "description": "组件关的依赖关系",
            "lock": true,
            "path": "dependencyList"
        }
    ],
    "pathConfig": {
        "dir": {
            "path": [
                "folder:{path} {key}"
            ]
        },
        "lang": {
            "path": [
                "folder:{path}Windows\\System32\\{key}"
            ]
        },
        "search": [
            "HKEY_LOCAL_MACHINE\\COMPONENTS\\DerivedData\\Components",
            "HKEY_LOCAL_MACHINE\\COMPONENTS\\DerivedData\\VersionedIndex\\{WinBuild}\\ComponentFamilies",
            "HKEY_LOCAL_MACHINE\\SOFTWARE\\Microsoft\\Windows\\CurrentVersion\\SideBySide\\Winners"
        ],
        "sys64": {
            "path": [
                "{path}ProgramData\\Microsoft\\Windows\\Start Menu\\ {key}?.",
                "{path}ProgramData\\Microsoft\\UEV\\ {key}",
                "parent:{path}Windows\\System32\\ {key}",
                "{path}Windows\\System32\\ {key}*.mui",
                "{path}Windows\\SystemResources {key}"
            ]
        },
        "pkg64": {
            "path": [
                "{path}Windows\\servicing\\Packages\\ {key}",
                "{path}Windows\\System32\\CatRoot\\ {key}"
            ],
            "reg": [
                {
                    "path": "HKEY_LOCAL_MACHINE\\SOFTWARE\\Microsoft\\Windows\\CurrentVersion\\Component Based Servicing\\PackageIndex",
                    "search": [
                        "{key}",
                        "amd64"
                    ]
                },
                {
                    "path": "HKEY_LOCAL_MACHINE\\SOFTWARE\\Microsoft\\Windows\\CurrentVersion\\Component Based Servicing\\PackageDetect",
                    "search": [
                        "{key}",
                        "amd64"
                    ]
                }
            ]
        },
        "pkg32": {
            "path": [
                "{path}Windows\\servicing\\Packages\\ {key} wow64|x86",
                "{path}Windows\\SysWow64\\CatRoot\\ {key}"
            ],
            "reg": [
                {
                    "path": "HKEY_LOCAL_MACHINE\\SOFTWARE\\Microsoft\\Windows\\CurrentVersion\\Component Based Servicing\\PackageIndex",
                    "search": [
                        "{key}",
                        "wow64"
                    ]
                },
                {
                    "path": "HKEY_LOCAL_MACHINE\\SOFTWARE\\Microsoft\\Windows\\CurrentVersion\\Component Based Servicing\\PackageDetect",
                    "search": [
                        "{key}",
                        "wow64"
                    ]
                },
                {
                    "path": "HKEY_LOCAL_MACHINE\\SOFTWARE\\Microsoft\\Windows\\CurrentVersion\\Component Based Servicing\\PackageIndex",
                    "search": [
                        "{key}",
                        "x86"
                    ]
                },
                {
                    "path": "HKEY_LOCAL_MACHINE\\SOFTWARE\\Microsoft\\Windows\\CurrentVersion\\Component Based Servicing\\PackageDetect",
                    "search": [
                        "{key}",
                        "x86"
                    ]
                }
            ]
        },
        "sys32": {
            "path": [
                "parent:{path}Windows\\SysWOW64\\ {key}|{key}.mui"
            ]
        },
        "sxs64": {
            "path": [
                "folder:{path}Windows\\WinSxS\\ amd64 {key}",
                "{path}Windows\\WinSxS\\Manifests\\ amd64 {key}",
                "{path}Windows\\WinSxS\\FileMaps\\ {key}"
            ],
            "reg": [
                {
                    "path": "HKEY_LOCAL_MACHINE\\COMPONENTS\\DerivedData\\Components",
                    "key": null,
                    "search": [
                        "{key}",
                        "amd64"
                    ],
                    "command": "delete"
                },
                {
                    "path": "HKEY_LOCAL_MACHINE\\COMPONENTS\\DerivedData\\VersionedIndex\\{WinBuild}\\ComponentFamilies",
                    "key": null,
                    "search": [
                        "{key}",
                        "amd64"
                    ],
                    "command": "delete"
                },
                {
                    "path": "HKEY_LOCAL_MACHINE\\SOFTWARE\\Microsoft\\Windows\\CurrentVersion\\SideBySide\\Winners",
                    "key": null,
                    "search": [
                        "{key}",
                        "amd64"
                    ],
                    "command": "delete"
                }
            ]
        },
        "sxs32": {
            "path": [
                "folder:{path}Windows\\WinSxS\\x86 {key}",
                "folder:{path}Windows\\WinSxS\\wow64 {key}",
                "{path}Windows\\WinSxS\\Manifests\\x86 {key}",
                "{path}Windows\\WinSxS\\Manifests\\wow64 {key}"
            ],
            "reg": [
                {
                    "path": "HKEY_LOCAL_MACHINE\\COMPONENTS\\DerivedData\\Components",
                    "key": null,
                    "search": [
                        "{key}",
                        "wow64"
                    ],
                    "command": "delete"
                },
                {
                    "path": "HKEY_LOCAL_MACHINE\\COMPONENTS\\DerivedData\\VersionedIndex\\{WinBuild}\\ComponentFamilies",
                    "key": null,
                    "search": [
                        "{key}",
                        "wow64"
                    ],
                    "command": "delete"
                },
                {
                    "path": "HKEY_LOCAL_MACHINE\\SOFTWARE\\Microsoft\\Windows\\CurrentVersion\\SideBySide\\Winners",
                    "key": null,
                    "search": [
                        "{key}",
                        "wow64"
                    ],
                    "command": "delete"
                },
                {
                    "path": "HKEY_LOCAL_MACHINE\\COMPONENTS\\DerivedData\\Components",
                    "key": null,
                    "search": [
                        "{key}",
                        "x86"
                    ],
                    "command": "delete"
                },
                {
                    "path": "HKEY_LOCAL_MACHINE\\COMPONENTS\\DerivedData\\VersionedIndex\\{WinBuild}\\ComponentFamilies",
                    "key": null,
                    "search": [
                        "{key}",
                        "x86"
                    ],
                    "command": "delete"
                },
                {
                    "path": "HKEY_LOCAL_MACHINE\\SOFTWARE\\Microsoft\\Windows\\CurrentVersion\\SideBySide\\Winners",
                    "key": null,
                    "search": [
                        "{key}",
                        "x86"
                    ],
                    "command": "delete"
                }
            ]
        }
    },
    "componentList": [
        {
            "name": "Test Unit",
            "display": "测试组件集",
            "description": "该项目为预览版测试专用，请谨慎使用。",
            "componentList": [
                {
                    "name": "Regedit",
                    "display": "注册表编辑器",
                    "description": "查看、编辑和管理Windows注册表的工具。",
                    "arch": {
                        "type": "intel",
                        "bit": 64
                    },
                    "path": {
                        "e": [
                            "parent:{path}Windows\\ regedit"
                        ],
                        "sys64": [
                            "regedit"
                        ],
                        "sxs64": [
                            "registry-editor"
                        ]
                    },
                    "componentList": [
                        {
                            "name": "Regedit - 32-bit",
                            "display": "注册表编辑器",
                            "arch": {
                                "type": "intel",
                                "bit": 32
                            },
                            "path": {
                                "sys32": [
                                    "regedit"
                                ],
                                "sxs32": [
                                    "registry-editor"
                                ]
                            }
                        }
                    ]
                },
                {
                    "name": "许可",
                    "description": null,
                    "path": {
                        "dir": [
                            "Windows\\System32\\ Licenses"
                        ]
                    }
                }
            ]
        },
        {
            "name": "System",
            "display": "系统",
            "description": "系统组件",
            "componentList": [
                {
                    "name": "MsftEdit Control",
                    "display": "富文本编辑控件",
                    "description": "为软件提供文本编辑控件。",
                    "componentList": [
                        {
                            "name": "Notepad",
                            "display": "记事本",
                            "description": "Windows 内置的文本编辑器，用于创建和编辑文本文件。",
                            "arch": {
                                "type": "intel",
                                "bit": 64
                            },
                            "path": {
                                "sys64": [
                                    "notepad"
                                ],
                                "sxs64": [
                                    "notepad"
                                ]
                            }
                        },
                        {
                            "name": "WordPad",
                            "display": "写字板",
                            "arch": {
                                "type": "intel",
                                "bit": 64
                            },
                            "path": {
                                "dir": [
                                    "Program Files\\Windows NT\\Accessories"
                                ],
                                "sys64": [
                                    "write.exe",
                                    "wordpad"
                                ],
                                "sxs64": [
                                    "wordpad"
                                ],
                                "pkg64": [
                                    "wordpad"
                                ]
                            }
                        },
                        {
                            "name": "MsftEdit Control - 32-bit",
                            "display": "富文本编辑控件",
                            "componentList": [
                                {
                                    "name": "Notepad - 32-bit",
                                    "display": "记事本",
                                    "arch": {
                                        "type": "intel",
                                        "bit": 32
                                    },
                                    "path": {
                                        "sys32": [
                                            "notepad"
                                        ],
                                        "sxs32": [
                                            "notepad"
                                        ]
                                    }
                                },
                                {
                                    "name": "WordPad - 32-bit",
                                    "display": "写字板",
                                    "arch": {
                                        "type": "intel",
                                        "bit": 32
                                    },
                                    "path": {
                                        "dir": [
                                            "Program Files (x86)\\Windows NT\\Accessories"
                                        ],
                                        "sys64": [
                                            "write.exe",
                                            "wordpad"
                                        ],
                                        "sxs32": [
                                            "wordpad"
                                        ],
                                        "pkg32": [
                                            "wordpad"
                                        ]
                                    }
                                }
                            ]
                        }
                    ]
                },
                {
                    "name": "Boot Immersive Menus",
                    "display": "沉浸式\"引导\"菜单",
                    "description": null,
                    "path": {
                        "sys64": [
                            "bootim",
                            "bootux"
                        ],
                        "sxs64": [
                            "bootux"
                        ]
                    }
                }
            ]
        }
    ],
    "isoComponentList": [
        {
            "name": "ISO镜像",
            "description": "ISO镜像中的组件。",
            "componentList": [
                {
                    "name": "Support",
                    "description": "一些辅助工具，不影响系统安装。",
                    "componentList": [
                        {
                            "name": "编码转换器(GB2U)",
                            "description": "GB18030-2000 与 Unicode相互转换的工具。",
                            "path": {
                                "e": [
                                    "support\\tools\\gbunicnv.exe"
                                ]
                            }
                        },
                        {
                            "name": "Windows Easy Transfer(migwiz)",
                            "description": "用户数据迁移工具",
                            "path": {
                                "dir": [
                                    "support\\migwiz"
                                ]
                            }
                        },
                        {
                            "name": "ETW Event Resources",
                            "description": "ETW事件监视资源。",
                            "path": {
                                "dir": [
                                    "support\\logging"
                                ]
                            }
                        }
                    ]
                },
                {
                    "name": "Upgrade",
                    "description": "包含从旧版本 Windows 升级所需的文件（如版本校验、兼容性检查等）。",
                    "path": {
                        "dir": [
                            "upgrade"
                        ]
                    },
                    "componentList": [
                        {
                            "name": ".Netfx",
                            "componentList": [
                                {
                                    "name": "NetFxUpdate",
                                    "path": {
                                        "e": [
                                            "upgrade\\netfx\\netfxupdate.exe"
                                        ]
                                    }
                                },
                                {
                                    "name": ".NET Framework 1.1",
                                    "path": {
                                        "e": [
                                            "upgrade\\netfx\\ *.msi",
                                            "upgrade\\netfx\\ *.msp",
                                            "upgrade\\netfx\\ *.cab"
                                        ]
                                    }
                                }
                            ]
                        }
                    ]
                },
                {
                    "name": "Sources",
                    "description": "Windows 安装目录",
                    "componentList": [
                        {
                            "name": "组件库(sxs)",
                            "path": {
                                "dir": [
                                    "sources\\sxs"
                                ]
                            }
                        },
                        {
                            "name": "版本配置(EI.CFG)",
                            "description": "此选项移除 Sources目录下的 ei.cfg文件，如果删除此文件并且最终映像中包含多个 Windows 版本，安装过程中会出现 Windows 版本选择对话框，如果安装过程中出现提示找不到用户许可协议之类的错误，请从原始光盘中恢复此文件，通常即可解决问题.",
                            "path": {
                                "e": [
                                    "sources\\EI.CFG"
                                ]
                            }
                        },
                        {
                            "name": "许可声明",
                            "path": {
                                "dir": [
                                    "sources\\license"
                                ]
                            }
                        }
                    ]
                },
                {
                    "name": "安装程序(setup.exe)",
                    "path": {
                        "e": [
                            "setup.exe",
                            "autorun.inf"
                        ]
                    }
                },
                {
                    "name": "Memory Diagnostic",
                    "description": "内存诊断",
                    "path": {
                        "e": [
                            "boot\\memtest"
                        ]
                    }
                },
                {
                    "name": "引导扇区操作工具",
                    "path": {
                        "e": [
                            "boot\\bootsect"
                        ]
                    }
                }
            ]
        }
    ],
    "Cleanup": [
        {
            "path": [
                "parent:{path} Windows\\System32\\config\\ *.LOG1",
                "parent:{path} Windows\\System32\\config\\ *.LOG2"
            ]
        }
    ]
}