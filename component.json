    "pathConfig": {
        "dir": {
            "path": [
                "folder:{path} {key}"
            ]
        },
        "lang": {
            "path": [
                "folder:{path}Program Files\\Common Files\\microsoft shared\\ink {key}",
                "folder:parent:{path}Windows\\ {key}",
                "folder:{path}Windows\\Boot\\EFI {key}",
                "folder:{path}Windows\\Boot\\EFI_EX {key}",
                "folder:{path}Windows\\Boot\\PCAT {key}",
                "folder:{path}Windows\\System32 {key}",
                "folder:{path}Windows\\SysWOW64 {key}",
                "{path}Windows\\SystemApps {key}",
                "{path}Windows\\BitLockerDiscoveryVolumeContents {key}"
            ],
            "reg": [
                {
                    "path": "HKEY_LOCAL_MACHINE\\SOFTWARE\\Microsoft\\Windows\\CurrentVersion\\SideBySide\\Winners",
                    "search": [
                        "{key}"
                    ],
                    "command": "delete"
                }
            ]
        },
        "search": [
            "HKEY_LOCAL_MACHINE\\SYSTEM\\ControlSet001\\Services",
            "HKEY_LOCAL_MACHINE\\COMPONENTS\\DerivedData\\Components",
            "HKEY_LOCAL_MACHINE\\COMPONENTS\\DerivedData\\VersionedIndex\\{WinBuild}\\ComponentFamilies",
            "HKEY_LOCAL_MACHINE\\SOFTWARE\\Microsoft\\Windows\\CurrentVersion\\SideBySide\\Winners",
            "HKEY_LOCAL_MACHINE\\SOFTWARE\\Microsoft\\Windows\\CurrentVersion\\Component Based Servicing\\PackageIndex",
            "HKEY_LOCAL_MACHINE\\SOFTWARE\\Microsoft\\Windows\\CurrentVersion\\Component Based Servicing\\PackageDetect"
        ],
        "startMenu":{
            "path": [
                "{path}ProgramData\\Microsoft\\Windows\\Start Menu\\Programs {key}.lnk"
            ]
        },
        "sys64": {
            "path": [
                "{path}ProgramData\\Microsoft\\UEV\\ {key}",
                "parent:{path}Windows\\System32\\ {key}",
                "{path}Windows\\System32\\ {key}*.mui",
                "{path}Windows\\SystemResources {key}"
            ]
        },
        "sys32": {
            "path": [
                "parent:{path}Windows\\SysWOW64\\ {key}",
                "{path}Windows\\SysWOW64\\ {key}*.mui"
            ]
        },
        "pkg64": {
            "path": [
                "{path}Windows\\servicing\\Packages\\ {key}",
                "{path}Windows\\System32\\CatRoot\\ {key}"
            ],
            "reg": [
                {
                    "path": "HKEY_LOCAL_MACHINE\\SOFTWARE\\Microsoft\\Windows\\CurrentVersion\\Component Based Servicing\\PackageIndex",
