    "itemList": [
        {
            "name": "组件名称",
            "description": "组件的内部名称或常用的表述形式。",
            "lock": true,
            "path": "name"
        },
        {
            "name": "显示名称",
            "description": "组件的译名。",
            "lock": false,
            "path": "display"
        },
        {
            "name": "组件描述",
            "description": "对组件功能的解释。",
            "lock": false,
            "path": "description"
        },
        {
            "name": "占用",
            "description": "组件的存储占用情况。",
            "lock": false,
            "path": "_size"
        },
        {
            "name": "架构",
            "description": "组件文件架构（显示'-'代表未知）。",
            "lock": false,
            "path": "arch.bit"
        },
        {
            "name": "兼容性",
            "description": "组件关联的兼容性信息",
            "lock": true,
            "path": "compatibilityList"
        },
        {
            "name": "依赖",
            "description": "组件关的依赖关系",
            "lock": true,
            "path": "dependencyList"
        }
